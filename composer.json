{
  "name": "fabpot/silex-skeleton",
  "description": "A pre-configured skeleton for the Silex microframework",
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.3.3",
    "silex/silex": "~1.0",
    "silex/web-profiler": "~1.0",
    "symfony/class-loader": "~2.3",
    "symfony/config": "~2.3",
    "symfony/console": "~2.3",
    "symfony/debug": "~2.3",
    "symfony/finder": "~2.3",
    "symfony/form": "~2.3",
    "symfony/monolog-bridge": "~2.3",
    "symfony/process": "~2.3",
    "symfony/security": "~2.3",
    "symfony/translation": "~2.3",
    "symfony/twig-bridge": "~2.3",
    "symfony/validator": "^2.8",
    "doctrine/dbal": "~2.2",
    "deralex/yaml-config-service-provider": "1.0.x-dev",
    "doctrine/orm": "^2.4",
    "fredjuvaux/doctrine-orm-provider": "1.0.*@dev",
    "pagerfanta/pagerfanta": "^1.1",
    "franmomu/silex-pagerfanta-provider": "^1.1"
  },
  "autoload": {
    "psr-0": {
      "": "src/"
    },
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "require-dev": {
    "symfony/browser-kit": "^2.8",
    "symfony/css-selector": "^2.8",
    "phpunit/phpunit": "^4.8"
  }
}
